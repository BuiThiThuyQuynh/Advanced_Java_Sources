package main;

public class TestSuperlativeEquation {
	
	/*
	 * Function: calculate equation of superlative equation (ax + b = 0)
	 * Input: a, b
	 * Output: x
	 */
	public static double calEquation (double a, double b) {
		if (a == 0)
			if (b == 0)
				return 0;
			else
				return b;
		else
			return -b / a;
	}

	public static void main(String[] args) {
		

	}
}
